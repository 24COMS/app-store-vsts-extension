{
    "manifestVersion": 1,
    "id": "ios-deploy-extension",
    "name": "Apple App Store",
    "version": "0.0.1",
    "publisher": "ms-vsclient",
    "description": "Streamline CI setup for your Apache Cordova, PhoneGap, Ionic, or Cordova CLI compatible app using a set of useful pre-defined build steps.",
    "categories": [
        "Build and release"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "visibility": [
        "Build and release"
    ],
    "icons": {
        "default": "",
        "large": ""
    },
    "content":{
        "details": {
            "path": "docs/extension-overview.md"
        },
        "thirdpartynotice": {
            "path": "ThirdPartyNotices.txt"
        }
    },
    "files": [
        {
            "path": "Tasks/IosStoreRelease"
        }
    ],
    "contributions": [
        {
            "id": "iosstorerelease",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": {
                "name": "Tasks/IosStoreRelease"
            }
        }
    ]
}
